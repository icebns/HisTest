<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>testweb</servlet-name>
        <servlet-class>com.icebns.test.TestWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testweb</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registerlistweb</servlet-name>
        <servlet-class>com.icebns.web.RegisterListWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerlistweb</servlet-name>
        <url-pattern>/registerlistweb</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>encoding</filter-name>
        <filter-class>com.icebns.web.EncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>encoding</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>putdata</servlet-name>
        <servlet-class>com.icebns.web.PutDataWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>putdata</servlet-name>
        <url-pattern>/putData</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>tuihao</servlet-name>
        <servlet-class>com.icebns.web.delRegisterWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tuihao</servlet-name>
        <url-pattern>/delRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getaregister</servlet-name>
        <servlet-class>com.icebns.web.GetRegisterWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getaregister</servlet-name>
        <url-pattern>/getRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getpay</servlet-name>
        <servlet-class>com.icebns.web.GetPayWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getpay</servlet-name>
        <url-pattern>/getPay</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getnopay</servlet-name>
        <servlet-class>com.icebns.web.GetNoPayWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getnopay</servlet-name>
        <url-pattern>/getNoPay</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>payit</servlet-name>
        <servlet-class>com.icebns.web.PayItWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>payit</servlet-name>
        <url-pattern>/payIt</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>putpay</servlet-name>
        <servlet-class>com.icebns.web.PutPayWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>putpay</servlet-name>
        <url-pattern>/putPay</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delpay</servlet-name>
        <servlet-class>com.icebns.web.delPayWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delpay</servlet-name>
        <url-pattern>/delPay</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.icebns.web.LoginWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>main</servlet-name>
        <servlet-class>com.icebns.web.LoginWeb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>


</web-app>
